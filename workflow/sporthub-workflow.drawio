<mxfile host="app.diagrams.net">
  <diagram name="SportHub Website Workflow">
    <mxGraphModel dx="1600" dy="900" grid="1" gridSize="10" guides="1"
      tooltips="1" connect="1" arrows="1" fold="1"
      page="1" pageScale="1" pageWidth="1200" pageHeight="900">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- USER -->
        <mxCell id="user" value="User&#10;(Web Browser)"
          style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;"
          vertex="1" parent="1">
          <mxGeometry x="470" y="40" width="260" height="80" as="geometry"/>
        </mxCell>

        <!-- FRONTEND -->
        <mxCell id="frontend" value="Web Application&#10;&#10;Frontend (React SPA)&#10;- Main&#10;- Login&#10;- Upload&#10;- Input&#10;- Reports"
          style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;"
          vertex="1" parent="1">
          <mxGeometry x="160" y="200" width="340" height="220" as="geometry"/>
        </mxCell>

        <!-- BACKEND -->
        <mxCell id="backend" value="Backend API&#10;&#10;Node.js + Express&#10;- Authentication&#10;- Business Logic&#10;- REST Endpoints"
          style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;"
          vertex="1" parent="1">
          <mxGeometry x="560" y="220" width="340" height="180" as="geometry"/>
        </mxCell>

        <!-- DATABASE -->
        <mxCell id="db" value="PostgreSQL Database"
          style="shape=cylinder;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;"
          vertex="1" parent="1">
          <mxGeometry x="360" y="470" width="300" height="120" as="geometry"/>
        </mxCell>

        <!-- EXTERNAL API -->
        <mxCell id="extapi" value="External Sports Data API"
          style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;"
          vertex="1" parent="1">
          <mxGeometry x="760" y="470" width="300" height="120" as="geometry"/>
        </mxCell>

        <!-- EDGES -->
        <mxCell id="e1" value="HTTP Requests"
          style="endArrow=block;html=1;"
          edge="1" parent="1" source="user" target="frontend">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="e2" value="REST API Calls"
          style="endArrow=block;html=1;"
          edge="1" parent="1" source="frontend" target="backend">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="e3" value="JSON Responses"
          style="endArrow=block;html=1;"
          edge="1" parent="1" source="backend" target="frontend">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="e4" value="Store / Retrieve Data"
          style="endArrow=block;html=1;"
          edge="1" parent="1" source="backend" target="db">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="e5" value="Fetch Sports Data"
          style="endArrow=block;html=1;dashed=1;"
          edge="1" parent="1" source="backend" target="extapi">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
